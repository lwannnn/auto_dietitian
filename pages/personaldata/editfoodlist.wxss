  padding: 20rpx 0;
}

.tiSet text {
  margin: 0 10rpx;
  padding: 15rpx 0;
  background: #58d487;
  border: 1px solid #58d487;
  color: #fff;
  box-sizing: border-box;
}

.sures {
  width: 80%;
  text-align: center;
  background: #58d487;
  color: #fff;
  position: fixed;
  bottom: 10rpx;
  left: 10%;
  padding: 10rpx 0;
  border-radius: 50rpx;
}

.suresNo {
  width: 80%;
  position: fixed;
  bottom: 10rpx;
  left: 10%;
  display: flex;
  justify-content: space-around;
}

.suresNo text {
  width: 40%;
  text-align: center;
  background: #58d487;
  color: #fff;
  padding: 12rpx 0;
  border-radius: 30rpx;
  font-size: 32rpx;
}

.over {
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
  align-content: center;
}

.overs {
  display: inline-block;
  width: 150rpx;
  padding-left: 20rpx;
  line-height: 38rpx;
}

