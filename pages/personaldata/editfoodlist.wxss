}

.paddtops {
  padding-top: 90rpx;
}

.orderList text {
  display: inline-block;
  width: 50%;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  padding: 10rpx 0;
  font-size: 32rpx;
  background: #fff;
}

.orderThree text {
  width: 33.33%;
  height: inherit;
}

.orderThree text:nth-of-type(2) {
